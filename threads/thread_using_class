from threading import *
from time import *

class Alphabets(Thread):
    def run(self):
        # method overriding 
        for i in range(65,91):
            print(chr(i))
            sleep(0.1)

t = Alphabets()
t.start()

for i in range(65,91):
    print(i)
    sleep(0.1)

# we want main program to wait for the t thread to finish
# otherwise main will exit
t.join()